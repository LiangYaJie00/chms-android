<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <RelativeLayout
        android:id="@+id/personal_postItem_btn"
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:paddingHorizontal="@dimen/normal_padding_16"
        android:gravity="center">

        <ImageView
            android:id="@+id/iv_start_icon"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"/>

        <TextView
            android:id="@+id/tv_container_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/iv_start_icon"
            android:layout_toEndOf="@id/iv_start_icon"
            android:text="健康报告"
            android:textSize="16sp"
            android:textColor="@color/black"
            android:layout_marginLeft="10dp"
            android:layout_centerVertical="true"/>

        <ImageView
            android:id="@+id/iv_end_icon"
            android:layout_width="7dp"
            android:layout_height="12dp"
            android:src="@mipmap/right"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="20dp"/>
    </RelativeLayout>
</merge>